import re
from os import system
from tqdm import tqdm


f=open('xab')
data=f.read()
f.close()


documents=re.findall('{.*?}',data,re.DOTALL)
documents[0]

for document in tqdm(documents):
    f=open('hn.json','w')

    for line in document.split('\n'):
        f.write(line)
        f.write('\n')

    f.close()

    system('curl -XPOST "localhost:9200/hn/document/?routing=$1&pretty" -d @hn.json')
